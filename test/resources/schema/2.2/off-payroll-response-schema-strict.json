{
  "publicId": "https://raw.githubusercontent.com/hmrc/off-payroll-decision/master/test/resources/schema/off-payroll-response-schema.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Decision Response Version 1.5.0-final",
  "definitions": {
    "weightedAnswer": {
      "type": "string",
      "enum": [
        "HIGH",
        "MEDIUM",
        "LOW",
        "CONTINUE",
        "NotValidUseCase",
        "OUTOFIR35",
        "INIR35"
      ]
    }
  },
  "type": "object",
  "properties": {
    "version": {
      "type": "string",
      "pattern": "^[0-9]{1,2}[.][0-9]{1,2}[.][0-9]{1,2}-?[a-zA-Z-_]*$"
    },
    "correlationID": {
      "type": "string"
    },
    "result": {
      "type": "string",
      "enum": [
        "Outside IR35",
        "Self-Employed",
        "Inside IR35",
        "Employed",
        "Unknown",
        "Not Matched"
      ]
    },
    "score": {
      "type": "object",
      "properties": {
        "setup": {
          "type": "string",
          "enum": [
            "CONTINUE",
            "NotValidUseCase"
          ]
        },
        "exit": {
          "type": "string",
          "enum": [
            "CONTINUE",
            "NotValidUseCase",
            "INIR35"
          ]
        },
        "personalService": {
          "$ref": "#/definitions/weightedAnswer"
        },
        "control": {
          "$ref": "#/definitions/weightedAnswer"
        },
        "financialRisk": {
          "$ref": "#/definitions/weightedAnswer"
        },
        "partAndParcel": {
          "$ref": "#/definitions/weightedAnswer"
        },
        "businessOnOwnAccount": {
          "$ref": "#/definitions/weightedAnswer"
        }
      },
      "additionalProperties": false,
      "required": [
        "setup",
        "exit",
        "personalService",
        "control",
        "financialRisk",
        "partAndParcel",
        "businessOnOwnAccount"
      ]
    }
  },
  "required": [
    "version",
    "correlationID",
    "score"
  ],
  "additionalProperties": false
}